[ANTINUKE_CLOUD_MAIN]{
	[cloud] {
		class=CSimpleParticleSystem;
		[properties]{
			sizeGrowth=4;
			sizeMod=1;
			pos=0, 0, 0;
			emitVector=0, 1, 0;
			gravity=0, 0, 0;
			colorMap=0 0 0 0  0.5 0.5 0.5 1  0.5 0.5 0.5 0.75  0.5 0.5 0.5 0.5  0 0 0 0;
			Texture=smokesmall;
			airdrag=0.98;
			particleLife=120;
			particleLifeSpread=30;
			numParticles=64;
			particleSpeed=8;
			particleSpeedSpread=1;
			particleSize=4;
			particleSizeSpread=4;
			emitRot=0;
			emitRotSpread=180;
			directional=0;
			alwaysVisible=1;
		}
		air=1;
		ground=1;
		water=1;
		count=1;
	}
}

