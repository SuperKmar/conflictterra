[ANTINUKE]{
	usedefaultexplosions=0;
	
	[cloud] {
		class=CExpGenSpawner;
		[properties]
		{
			dir=dir;
			pos=0, 0, 0; 
			delay=20;
			explosionGenerator=custom:ANTINUKE_CLOUD; 
		}
		air=1;
		ground=1;
		water=1;
		count=1;
	}
	
	[pikes] {
		class=explspike;
		[properties] {
			dir= -8 r16, -8 r16, -8 r16;
			width=64;
			length=1;
			lengthGrowth=1;
			alpha=1;
			alphadecay=0.006;
			color=1,0.5,0.1;
			alwaysVisible=1;
		}
		air=1;
		water=1;
		ground=1;
		count=32; 
	}
	
	[sphere] {
		class=CSpherePartSpawner;
		[properties] {
			expansionSpeed=15;
			ttl=80;
			alpha=0.5;
			color=1,1,0.5;
			alwaysVisible=1;
		}
		air=1;
		water=1;
		ground=1;
		count=1;
	}
}

